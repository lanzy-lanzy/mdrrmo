{
    "version": 2,
    "builds": [
      { "src": "mdrrmo/wsgi.py", "use": "@vercel/python" },
      { "src": "staticfiles/**", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/static/(.*)", "dest": "/staticfiles/$1" },
      { "src": "/(.*)", "dest": "mdrrmo/wsgi.py" }
    ]
  }
  